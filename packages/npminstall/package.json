    "agentkeepalive": "^4.0.2",
    "await-event": "^2.1.0",
    "bin-links": "^2.3.0",
    "binary-mirror-config": "^1.19.0",
    "black-hole-stream": "^0.0.1",
    "bytes": "^3.1.0",
    "chalk": "^2.4.2",
    "cmd-shim-hotfix": "^3.0.3",
    "debug": "^4.1.1",
    "destroy": "^1.0.4",
    "detect-libc": "^2.0.1",
    "fs-extra": "^7.0.1",
    "minimatch": "^3.0.4",
    "minimist": "^1.2.0",
    "moment": "^2.24.0",
    "ms": "^2.1.1",
    "node-gyp": "^9.0.0",
    "node-homedir": "^1.1.1",
    "normalize-package-data": "^2.5.0",
    "npm-normalize-package-bin": "^1.0.1",
    "npm-package-arg": "^8.1.5",
    "ora": "^4.0.5",
    "p-map": "^2.1.0",
    "pacote": "^13.0.0",
    "runscript": "^1.3.0",
    "semver": "^6.0.0",
    "tar": "^4.4.8",
    "urllib": "^3.0.3",
    "utility": "^1.16.1"
