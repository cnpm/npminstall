  "devDependencies": {
    "coffee": "5",
    "egg-bin": "5",
    "eslint": "8",
    "eslint-config-egg": "12",
    "git-contributor": "1",
    "mm": "3"
  },
  "homepage": "https://github.com/cnpm/npminstall",
  "repository": {
    "type": "git",
    "url": "git://github.com/cnpm/npminstall.git"
