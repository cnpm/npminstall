{
  "name": "npminstall",
  "version": "1.0.2",
  "description": "Let npm install fast and easy",
  "main": "lib/index.js",
  "files": [
    "lib",
    "bin"
  ],
  "bin": {
    "npminstall": "bin/install.js"
  },
  "scripts": {
    "test": "mocha -r thunk-mocha -t 120000 test/*.test.js",
    "test-cov": "istanbul cover node_modules/mocha/bin/_mocha -- -r thunk-mocha -t 120000 test/*.test.js",
    "test-local": "npm_registry=https://registry.npm.taobao.org mocha -r thunk-mocha -t 120000 test/*.test.js",
    "test-local-cov": "npm_registry=https://registry.npm.taobao.org istanbul cover node_modules/mocha/bin/_mocha -- -r thunk-mocha -t 120000 test/*.test.js",
    "lint": "eslint lib test bin",
    "ci": "npm run lint && npm run test-cov",
    "autod": "autod -w --prefix '~'"
  },
  "dependencies": {
    "await-event": "~1.0.0",
    "bytes": "~2.2.0",
    "chalk": "~1.1.1",
    "cmd-shim": "~2.0.1",
    "co": "~4.6.0",
    "co-fs-extra": "~1.2.0",
    "co-parallel": "~1.0.0",
    "debug": "~2.2.0",
    "destroy": "~1.0.4",
    "mkdirp": "~0.5.1",
    "ms": "~0.7.1",
    "mz": "~2.3.0",
    "node-gyp": "~3.2.1",
    "node-uuid": "~1.4.7",
    "npm-package-arg": "~4.1.0",
    "rimraf": "~2.5.1",
    "runscript": "~1.0.0",
    "semver": "~5.1.0",
    "tar": "~2.2.1",
    "urllib": "~2.7.1",
    "utility": "~1.6.0"
  },
  "devDependencies": {
    "autod": "*",
    "coffee": "3",
    "eslint": "1",
    "eslint-config-egg": "1",
    "istanbul": "*",
    "mm": "1",
    "mocha": "2",
    "thunk-mocha": "1"
  },
  "homepage": "https://github.com/cnpm/npminstall",
  "repository": {
    "type": "git",
    "url": "git://github.com/cnpm/npminstall.git",
    "web": "https://github.com/cnpm/npminstall"
  },
  "bugs": {
    "url": "https://github.com/cnpm/npminstall/issues",
    "email": "m@fengmk2.com"
  },
  "keywords": [
    "npminstall"
  ],
  "engines": {
    "node": ">=4.2.3"
  },
